@charset "UTF-8";
/* CSS Document */

body {
    margin: 20px; margin-bottom: 0px;
}

.enter_btn {
    color: white;
    padding: 7px;
    width: 150px;
    border-radius: 5px;
    font-size: 10pt;

    background: #3f629e;

    background: -moz-linear-gradient(top, #3f629e 0%, #4b7bbe 100%);

    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #3f629e), color-stop(100%, #4b7bbe));

    background: -webkit-linear-gradient(top, #3f629e 0%, #4b7bbe 100%);

    background: -o-linear-gradient(top, #3f629e 0%, #4b7bbe 100%);

    background: -ms-linear-gradient(top, #3f629e 0%, #4b7bbe 100%);

    background: linear-gradient(to bottom, #3f629e 0%, #4b7bbe 100%);

    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#3f629e', endColorstr='#4b7bbe', GradientType=0);

}

a.enter_btn:hover {
    color: white;
    text-decoration: none;

    background: #638e15;

    background: -moz-linear-gradient(top, #638e15 0%, #94c43d 100%);

    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #638e15), color-stop(100%, #94c43d));

    background: -webkit-linear-gradient(top, #638e15 0%, #94c43d 100%);

    background: -o-linear-gradient(top, #638e15 0%, #94c43d 100%);

    background: -ms-linear-gradient(top, #638e15 0%, #94c43d 100%);

    background: linear-gradient(to bottom, #638e15 0%, #94c43d 100%);

    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#638e15', endColorstr='#94c43d', GradientType=0);

}

.button_block {
    text-align: right;
    margin-bottom: 0px;
    margin-top: -10px;
}

.layer a {

    color: white;
    font-size: 12pt;
    padding: 7px;
    text-decoration: none;
    width: 140px;
    height: 36px;
    margin-right: 1px;
    display: inline-block;
    text-align: center;
    text-transform: uppercase;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;

    background: #94c43d;

    background: -moz-linear-gradient(top, #94c43d 0%, #94c43d 55%, #638e15 100%);

    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #94c43d), color-stop(55%, #94c43d), color-stop(100%, #638e15));

    background: -webkit-linear-gradient(top, #94c43d 0%, #94c43d 55%, #638e15 100%);

    background: -o-linear-gradient(top, #94c43d 0%, #94c43d 55%, #638e15 100%);

    background: -ms-linear-gradient(top, #94c43d 0%, #94c43d 55%, #638e15 100%);

    background: linear-gradient(to bottom, #94c43d 0%, #94c43d 55%, #638e15 100%);

    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#94c43d', endColorstr='#638e15', GradientType=0);

}

.layer a.active {
    background: #94c43d;

    background: -moz-linear-gradient(top, #94c43d 0%, #94c43d 100%);

    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #94c43d), color-stop(100%, #94c43d));

    background: -webkit-linear-gradient(top, #94c43d 0%, #94c43d 100%);

    background: -o-linear-gradient(top, #94c43d 0%, #94c43d 100%);

    background: -ms-linear-gradient(top, #94c43d 0%, #94c43d 100%);

    background: linear-gradient(to bottom, #94c43d 0%, #94c43d 100%);

    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#94c43d', endColorstr='#94c43d', GradientType=0);

}

